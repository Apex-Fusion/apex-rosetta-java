#common env
DB_ADMIN_USER_NAME="rosetta_db_admin"
DB_ADMIN_USER_SECRET="weakpwd#123_d"
SERVICE_USER_NAME="rosetta_db_service_user"
SERVICE_USER_SECRET="weakerpwd#123_d"

DB_IMAGE_NAME="postgres"
DB_IMAGE_TAG="latest"
DB_NAME="rosetta"
DB_HOST="db"
DB_PORT="5432"
DB_SCHEMA="preprod"
MAXIMUM_POOL_SIZE=80
