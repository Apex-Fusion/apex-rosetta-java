SUBMIT_SOCKET_CONNECTION=/ipc/node.socket
NETWORK_MAGIC=1
TRANSACTION_TTL=3000
NETWORK=preprod

DB_CONNECTION_PARAMS_PROVIDER_TYPE="ENVIRONMENT"
